{% extends 'base.html.twig' %}

{% block title %}Hello UserProfilController!{% endblock %}

{% block body %}

{{ form_start(form) }}
    {{ form_errors(form) }}

    {{ form_row(form.username) }}
    {{ form_row(form.first_name) }}
    {{ form_row(form.last_name) }}
    {{ form_row(form.email) }}
    {{ form_row(form.city) }}
    {{ form_row(form.password) }}
    <input type="submit" class="btn btn-primary">
    <a class="btn btn-primary" href="{{ path('user_profil') }}">Annuler</a>

{{ form_end(form) }}

{% endblock %}
